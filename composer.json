{
  "name": "elnooronline/laravel-notifications",
  "description": "Imagine having an minimal abstract layer for sending notifications instead of Laravel's",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdallah Mohammed",
      "email": "abdallah.r660@gmail.com"
    },
    {
      "name": "Elnooronline",
      "email": "elnooronline@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1",
    "laravel/framework": "^7.0|^8.0|^9.0",
    "laravel-notification-channels/fcm": "^3.2",
    "phlak/semver": "^4.1"
  },
  "require-dev": {
    "orchestra/testbench": "~6.0|~7.0",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Elnooronline\\Notifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elnooronline\\Notifications\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Elnooronline\\Notifications\\Providers\\NotificationsServiceProvider"
      ]
    }
  }
}
